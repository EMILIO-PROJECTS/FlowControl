#include <iostream>

using namespace std;



int main() {

    system ("cls");

    int a = 8;
    int b = 5;


    cout << "max: " << ((a > b) ? a : b) << endl;
    cout << "min: " << ((a < b) ? a : b) << endl;

    
    auto min = (a > b) ? a : 22.5f;

    cout << "min: " << min << endl;


}